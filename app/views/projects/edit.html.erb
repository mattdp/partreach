<% provide(:title, "Edit project #{@project.name}") %>

<div class="row">
  <div class="col-md-12">
    <div class="col-md-4"></div>
    <div id="any-form" class="col-md-4">
      <%= render partial: "projects/go_back", locals: {comment: @comment} %>
      <h1>Edit project '<%= "#{@project.name}" %>'</h1>
      <%= form_tag(project_path, method: 'patch') do %>
        <%= render partial: "projects/new_and_edit_form", locals: {project: @project} %>
      <% end %>
    </div>
  </div>
</div>