<h1>Gallery of additive manufacturers in the US</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Url_main</th>
    <th>Url_materials</th>
    <th>Blurb</th>
    <th>Email</th>
    <th>Phone</th>
    <th>Address ID</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @suppliers.each do |supplier| %>
  <tr>
    <td><%= supplier.name %></td>
    <td><%= link_to supplier.url_main, url_with_protocol(supplier.url_main), :target => '_blank' %></td>
    <td><%= link_to supplier.url_materials, url_with_protocol(supplier.url_materials), :target => '_blank' %></td>
    <td><%= supplier.blurb %></td>
    <td><%= supplier.email %></td>
    <td><%= supplier.phone %></td>
    <td><%= supplier.address_id %></td>        
    <td><%= link_to 'Show', supplier %></td>
    <td><%= link_to 'Edit', edit_supplier_path(supplier) %></td>
    <td><%= link_to 'Destroy', supplier, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Supplier', new_supplier_path %>
