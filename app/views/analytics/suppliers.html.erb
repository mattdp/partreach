<% provide(:title, "Suppliers") %>

<div class="container">
  <div class="row">
    <div class="col-sm-10 col-sm-offset-1 main-area-contrast override">

      <table class="supplier-list">
        <tr>
          <th>Status</th>
          <th>ID</th>
          <th>Name</th>
          <th>Next contact date</th>
          <th>Next contact content</th>
        </tr>
        <% @listings.each do |k,v| %>
          <tr>
            <td><%= "#{k} #{v.count}" %></td>
          </tr>
          <% v.each do |supplier| %>
            <%= render :partial => "analytics/supplier_listing", locals: {supplier: supplier} %>
          <% end %>
        <% end %>
      </table>

      <%= render 'analytics/links' %>

    </div>
  </div>
</div>