<% provide(:title,"Tag index") %>

<div class="container">
  <div class="row">
  	<div class="col-sm-10 col-sm-offset-1 main-area-contrast override">
  		<%= link_to "Create new tag", new_tag_path %>
  		<table>
	  		<% @tags.each do |tag| %>
          <tr>
            <td><%= tag.readable %></td>
	  			  <td><%= link_to "Edit", edit_tag_path(tag) %></td>
	  		<% end %>
  		</table>
  	</div>
  </div>
</div>