<div class="row">
  <div class="col-md-3">
    <p><%= link_to "Back to search page", teams_index_path %></p>
  </div>
  <div class="col-md-6 center">
    <p><%= middle_text %></p>
  </div>

  <% if display_options = "providers_list" %>
    <div class="col-md-2 right">          
      <p><%= link_to "Add vendor", new_provider_path %></p>
    </div>
    <div class="col-md-1">          
      <p><%= link_to "All tags", organizations_tags_list_path(@organization.id)%></p>
    </div>
  <% elsif display_options = "tags_list" %>
    <div class="col-md-1 right">          
      <p><%= link_to "Add vendor", new_provider_path %></p>
    </div>
    <div class="col-md-2 right">          
      <p><%= link_to "All companies", organizations_providers_list_path(@organization.id)%></p>
    </div>
  <% elsif display_options = "profile" %>
    <div class="col-md-2 right">          
      <p><%= link_to "All companies", organizations_providers_list_path(@organization.id)%></p>
    </div>
    <div class="col-md-1">          
      <p><%= link_to "All tags", organizations_tags_list_path(@organization.id)%></p>
    </div>
  <% else %>
    <div class="col-md-2 right">          
      <p><%= link_to "Add vendor", new_provider_path %></p>
    </div>
  <% end %>


</div>