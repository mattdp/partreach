  <% if score > 0 %>
    <% if include_hidden_number %>
      <span class="hide-text"><%= score %></span>
    <% end %>
    <span class="star-rating comment-rating-stars">
      <% 5.times do |num| %>
       <input type="radio">
      <% end %>
      <%#= search "TERRIBLE NOT DRY WAY TO RENDER" in css to see rationale for below %>
      <% score.times do |num| %>
        <i id="gold-star"></i>
      <% end %>
    </span>
  <% end %>
